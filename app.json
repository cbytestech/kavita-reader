      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.webtechnick.kavitareader"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "45df5044-5be7-4201-8f7b-760737b9f86d"
      }
    }
